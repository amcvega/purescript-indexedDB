{
    "extends": "airbnb",
    "env": {
        "commonjs": true
    },
    "parserOptions": {
        "ecmaVersion": 5
    },
    "globals": {
        "indexedDB": true,
        "PS": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-param-reassign": 0,
        "import/no-unresolved": 0,
        "no-underscore-dangle": 0,
        "prefer-template": 0
    }
}
